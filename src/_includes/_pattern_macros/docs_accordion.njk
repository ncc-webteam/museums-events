          <li>
            <a class="{% if '/getting-started/browsers-and-devices' in currenturl %}active{% endif %}" href="{{ '/getting-started/browsers-and-devices' | url }}">Browsers and devices</a>
          </li>
          <li>
            <a class="{% if '/getting-started/accessibility' in currenturl %}active{% endif %}" href="{{ '/getting-started/accessibility' | url }}">Accessibility</a>
          </li>
          <li>
            <a class="{% if '/getting-started/markdown-guide' in currenturl %}active{% endif %}" href="{{ '/getting-started/markdown-guide' | url }}">Markdown guide</a>
          </li>
        </ul>
      </div>
    </div>
  </div>

