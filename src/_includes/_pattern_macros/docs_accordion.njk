{% macro docsaccordion(currenturl) %}

<div class="accordion accordion-flush" id="docsAccordion">
  <div class="accordion-item">
    <h2 class="accordion-header" id="headingOne">
      {% if '/getting-started/' in currenturl %}
      <button
        id="headingOneButton"
        class="accordion-button"
        type="button"
        data-bs-toggle="collapse"
        data-bs-target="#collapseOne"
        aria-expanded="true"
        aria-controls="collapseOne">
        Getting Started
      </button>
      {% else %}
      <button
        id="headingOneButton"
        class="accordion-button collapsed"
        type="button"
        data-bs-toggle="collapse"
        data-bs-target="#collapseOne"
        aria-expanded="false"
        aria-controls="collapseOne">
        Getting Started
      </button>
      {% endif %}
    </h2>
    <div id="collapseOne" class="accordion-collapse collapse {% if '/getting-started/' in currenturl %}show{% endif %}" aria-labelledby="headingOne">
      <div class="accordion-body">
        <ul class="accordion-list">
          <li>
            <a id="getting-started-link" class="docs-accordion-link {% if '/getting-started/' == currenturl %}active{% endif %}" href="{{ '/getting-started' | url }}">How to get started</a>
          </li>
          <li>
            <a id="developer-environment-setup-link" class="docs-accordion-link {% if '/developer-environment-setup/' in currenturl %}active{% endif %}"  href="{{ '/getting-started/developer-environment-setup' | url }}">Developer environment setup</a>
